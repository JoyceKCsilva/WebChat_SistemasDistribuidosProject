# Configurações do servidor
PORT=8080
NODE_ENV=development

# Configurações do MQTT
MQTT_HOST=localhost
MQTT_PORT=1883
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_CLIENT_ID_PREFIX=forum-backend

